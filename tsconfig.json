{
  "include": ["src"],
  "compilerOptions": {
    "target": "ES2019",
    "module": "esnext",
    "jsx": "react",
    "jsxFactory": "h",
    "outDir": "dist",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "sourceMap": true
  }
}
